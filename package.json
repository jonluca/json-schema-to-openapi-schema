{
	"name": "browser-json-schema-to-openapi-schema",
	"version": "0.0.3",
	"description": "Converts a JSON Schema to OpenAPI Schema Object",
	"type": "module",
	"bin": {
		"json-schema-to-openapi-schema": "bin/json-schema-to-openapi-schema.js"
	},
	"types": "dist/mjs/index.d.ts",
	"files": [
		"/dist"
	],
	"main": "dist/cjs/index.js",
	"module": "dist/mjs/index.js",
	"exports": {
		".": {
			"import": "./dist/mjs/index.js",
			"require": "./dist/cjs/index.js"
		}
	},
	"scripts": {
		"build": "rm -fr dist/* && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && node scripts/fixup.cjs",
		"typecheck": "tsc --noEmit",
		"test": "vitest test/**",
		"coverage": "vitest --coverage"
	},
	"repository": "github:openapi-contrib/json-schema-to-openapi-schema",
	"author": "OpenAPI Contrib",
	"license": "MIT",
	"engines": {
		"node": ">=10"
	},
	"dependencies": {
		"@apidevtools/json-schema-ref-parser": "^9.0.9",
		"@cloudflare/json-schema-walker": "^0.1.1",
		"chalk": "^5.0.1",
		"clone": "^2.1.2",
		"yargs": "^17.5.1"
	},
	"devDependencies": {
		"@types/clone": "^2.1.1",
		"@typescript-eslint/eslint-plugin": "^5.31.0",
		"@typescript-eslint/parser": "^5.31.0",
		"c8": "^7.12.0",
		"coveralls": "^3.1.1",
		"eslint": "^8.20.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-unused-imports": "^2.0.0",
		"mocha": "^10.0.0",
		"nock": "^13.2.9",
		"should": "^13.2.3",
		"typescript": "^4.7.4",
		"vitest": "^0.20.2"
	},
	"prettier": {
		"singleQuote": true,
		"useTabs": true
	}
}
